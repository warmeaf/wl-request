{
  "name": "wl-request",
  "version": "0.1.0",
  "description": "一个基于 TypeScript 的现代化请求库，采用分层架构设计",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.15.3",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "commitlint": "commitlint --edit",
    "dev": "vite",
    "build": "pnpm -r build",
    "build:vite": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "request",
    "http",
    "fetch",
    "axios",
    "adapter",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "4.0.0-beta.19",
    "@vitest/ui": "4.0.0-beta.19",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "only-allow": "^1.2.2",
    "typescript": "^5.9.3",
    "vite": "8.0.0-beta.0",
    "vitest": "4.0.0-beta.19"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
